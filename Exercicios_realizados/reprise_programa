#include <stdio.h>
#include <math.h>

int main()
{
/* -----------------------ENTRADA DE DADOS-------------------------------------------*/   
 
 	float total; 
 	 
 	char nomePrograma[60];
 	 
 	printf ("Digite o nome do programa: ");
 	scanf("%s", &nomePrograma);
	 	
 	printf ("Digite o resultado da pesquisa: ");
 	scanf ("%f", &total);
 	 
 
/*----------------------------PROCESSO------------------------------------------------*/
 	 if (total >= 0 && total <= 100){
	  
	  		if (total > 50 && total <= 100){
			  
				printf ("O programa %s sera reprisado no horario nobre",nomePrograma);			  
			  
			  
		   }if (total > 25 && total <= 50){
		   
		   		printf ("O programa %s sera reprisado no horario diferente",nomePrograma);
		   
		   
		   }if (total <= 25){
		   
		   		printf ("O programa %s naos sera reprisado",nomePrograma);
		   }
		   
	  
	  }else{
	  
	   printf ("numero invalido, digite um valor de [1...100]");
}
/*---------------------------SAÍDA DE DADOS-------------------------------------------*/
	  
 	return 0;
}